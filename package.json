{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "Javascript Development Environment",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "yarn run lint --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain jsdev",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -R buildScripts/testSetup.js \"src/**/*.test.js\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls && rimraf ./coverage",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "yarn test -w",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "yarn run generate-mock-data",
    "start-mockapi": "json-server src/api/db.json --port 3001 -w",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "main": "index.js",
  "repository": "https://github.com/barryblando/js-dev-env.git",
  "author": "Retr0_0x315 <barryblando@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "raven-js": "^3.23.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chalk": "^2.3.2",
    "cheerio": "^1.0.0-rc.2",
    "circular-dependency-plugin": "^5.0.1",
    "compression": "^1.7.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-watch": "^3.1.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.0.7",
    "istanbul": "^0.4.5",
    "jsdom": "^11.6.2",
    "json-schema-faker": "^0.5.0-rc13",
    "localtunnel": "^1.8.3",
    "mocha": "^5.0.4",
    "nock": "^9.2.3",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.2.1",
    "numeral": "^2.0.6",
    "open": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "surge": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.2.0",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-md5-hash": "^0.0.6"
  }
}
